{
  "name": "@davidegheri/lru",
  "version": "1.0.1",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "module": "dist/esm/index.js",
  "jsnext:main": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "author": "Davide Gheri <davide@davidegheri.com>",
  "bugs": {
    "url": "https://github.com/Davide-Gheri/lru/issues"
  },
  "homepage": "https://github.com/Davide-Gheri/gm-palette#readme",
  "license": "MIT",
  "scripts": {
    "build:cjs": "tsc -p tsconfig.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "rimraf dist && yarn build:cjs && yarn build:esm && rollup -c rollup.config.js",
    "test": "jest",
    "test:coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "preversion": "yarn lint && yarn test",
    "version": "yarn build",
    "postversion": "git push --follow-tags"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "coveralls": "^3.0.3",
    "husky": "^3.1.0",
    "jest": "^24.8.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.10.8",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
